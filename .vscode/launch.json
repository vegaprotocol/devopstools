    "version": "0.2.0",
    "configurations": [
        {
            "name": "vegatools backup backup",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "backup", "backup",
            ],
            "console": "integratedTerminal",
            "asRoot": true,
        },
        {
            "name": "vegatools backup restore",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "backup", "restore",
                "--id", "xadas"
            ],
            "console": "integratedTerminal",
            "asRoot": true,
        }
        
    ]
}